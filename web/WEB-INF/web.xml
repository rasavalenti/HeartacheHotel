<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AccessServlet</servlet-name>
        <servlet-class>AccessDB.AccessServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AccessServlet</servlet-name>
        <url-pattern>/AccessServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CheckDates</servlet-name>
        <servlet-class>AccessDB.CheckDates</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckDates</servlet-name>
        <url-pattern>/CheckDates</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>HotelReception</servlet-name>
        <servlet-class>AccessDB.HotelReception</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HotelReception</servlet-name>
        <url-pattern>/HotelReception</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ManageBooking</servlet-name>
        <servlet-class>AccessDB.ManageBooking</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ManageBooking</servlet-name>
        <url-pattern>/ManageBooking</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RoomStatus</servlet-name>
        <servlet-class>AccessDB.RoomStatus</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RoomStatus</servlet-name>
        <url-pattern>/RoomStatus</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AddRooms</servlet-name>
        <servlet-class>AccessDB.AddRooms</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddRooms</servlet-name>
        <url-pattern>/AddRooms</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
